"xbacklight -dec 2"
  XF86MonBrightnessDown
"xbacklight -inc 2"
  XF86MonBrightnessUp

# Increase volume
# "pactl set-sink-volume 0 +10%"
"amixer set Master 5%+"
   XF86AudioRaiseVolume

# Decrease volume
# "pactl set-sink-volume 0 -10%"
"amixer set Master 5%-"
   XF86AudioLowerVolume

# Mute/unmute volume
# "pactl set-sink-mute 0 toggle"
"amixer set Master toggle"
   XF86AudioMute

# Mute/unmute mic
"amixer set Capture toggle"
   XF86AudioMicMute

